def reverse(nums , i , j):
    while i < j:
        nums[i] , nums[j] = nums[j] , nums[i]
        i+=1
        j-=1
        


def rotate(nums:list , k:int) -> list:
    k = k % len(nums)
    reverse(nums , 0,len(nums)-1) # ro'yxatni to'liq aylantradi 
    reverse(nums , 0 , k-1) #ro'yxat boshidan k gacha aylantramiz [5,6,7]
    reverse(nums , k ,len(nums)-1) #k dan royxat oxirgacha aylantramiz [1,2,3,4]
    return nums
son = [1,2,3,4,6,7,8]
print(rotate(son,3))